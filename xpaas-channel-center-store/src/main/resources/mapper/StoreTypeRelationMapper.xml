<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deepexi.channel.mapper.StoreTypeRelationMapper">
    <resultMap type="com.deepexi.channel.domain.StoreTypeRelationDO" id="StoreTypeRelationDOResult">
        <result property="id" column="id" />
        <result property="storeTypeId" column="store_type_id" />
        <result property="storeId" column="store_id" />
        <result property="description" column="description" />
        <result property="version" column="version" />
        <result property="remark" column="remark" />
        <result property="dr" column="dr" />
        <result property="createdBy" column="created_by" />
        <result property="createdTime" column="created_time" />
        <result property="updatedBy" column="updated_by" />
        <result property="updatedTime" column="updated_time" />
        <result property="appId" column="app_id" />
        <result property="tenantId" column="tenant_id" />
    </resultMap>
    <select id="findAll" resultType="com.deepexi.channel.domain.StoreTypeRelationDO">
        select * from cc_store_type_relation where dr = 0
        <if test="id != null">
            and id = #{id}
        </if>
        <if test="storeTypeId != null">
            and store_type_id = #{storeTypeId}
        </if>
        <if test="storeId != null">
            and store_id = #{storeId}
        </if>
        <if test="storeTypeIds != null and storeTypeIds.size() > 0">
            and store_type_id
            in
            <foreach collection="storeTypeIds" item="tempId" index="index" open="(" close=")" separator=",">
                #{tempId}
            </foreach>
        </if>

    </select>
</mapper>