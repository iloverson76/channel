<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deepexi.channel.mapper.CcStoreAreaMapper">
    <resultMap type="com.deepexi.channel.domain.dto.CcStoreAreaDto" id="CcStoreAreaDtoResult">
        <result property="id" column="id" />
        <result property="areaId" column="area_id" />
        <result property="storeId" column="store_id" />
        <result property="version" column="version" />
        <result property="remark" column="remark" />
        <result property="dr" column="dr" />
        <result property="createdBy" column="created_by" />
        <result property="createdTime" column="created_time" />
        <result property="updatedBy" column="updated_by" />
        <result property="updatedTime" column="updated_time" />
        <result property="appId" column="app_id" />
        <result property="tenantId" column="tenant_id" />
    </resultMap>
    <select id="findList" resultType="com.deepexi.channel.domain.eo.CcStoreArea">
        SELECT * FROM cc_store_area WHERE 1=1
    </select>

    <update id="deleteByIds">
        UPDATE cc_store_area set dr=1 where id in
        <foreach collection="ids" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </update>
</mapper>